<!DOCTYPE html>
<html lang="en">
  <head>
		<!-- Global site tag (gtag.js) - Google Analytics -->
		<script async src="https://www.googletagmanager.com/gtag/js?id=G-RX0F2Z43MF"></script>
		<script>
		  window.dataLayer = window.dataLayer || [];
		  function gtag(){dataLayer.push(arguments);}
		  gtag('js', new Date());

		  gtag('config', 'G-RX0F2Z43MF');
		</script>
  	<meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>{{title}}</title> 
    <meta name="description" content="">
	  <meta name="viewport" content="width=device-width, initial-scale=1">

	  <meta property="og:title" content="{{title}}">
	  <meta property="og:type" content="website" />
	  <meta property="og:url" content="{{url}}">
	  <meta property="og:image" content="{{image}}">
	  <meta property="og:description" content="{{META_DESCRIPTION}}">

  	<link rel="apple-touch-icon" href="/images/icon.png">
  	<link rel="preconnect" href="https://fonts.gstatic.com">
		<link href="https://fonts.googleapis.com/css2?family=Roboto+Condensed:wght@700&family=Roboto:wght@400;700&display=swap" rel="stylesheet">
    <link rel='stylesheet' href='/stylesheets/styles.css' />
    <link rel="stylesheet" type="text/css" href="//cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.css"/>
  </head>
  <body>
  	<div id="main" class="grid-container full">
		  <div class="grid-x">
		  	<div class="cell">

		  		<div class="module-mobile-nav hide-for-large">
		        {{> nav}}
		      </div>

		  		{{!- main container --}}
		  		<div class="grid-container main-container">
		  			<div class="grid-x">
		  				<div class="cell">

		  					<header>
						  		<div class="grid-container container-inner">
									  <div class="grid-x small-header hide-for-large align-justify">
									  	<div class="cell small-8 medium-6 flex-middle">
									  		<a href="/">
									  			<img src="/images/Joann-Ariola-For-City-Council-Logo.png" class="logo" alt="Joann Ariola For City Council - District 32">
									  		</a>
									  	</div>
									  	<div class="cell small-4 medium-2">
									  		
									  		<button id="module-nav-hamburger" class="module-nav-hamburger hide-for-large" aria-label="Menu" role="button" aria-controls="navigation" aria-expanded="true/false">
									        <span>toggle menu</span>
									      </button>

									  	</div>
									  </div>
									  <div class="grid-x">
									  	<div class="cell logo-outer show-for-large">
									    	<div class="logo-container">
									    		<a href="/">
									    			<img src="/images/Joann-Ariola-For-City-Council-Logo.png" class="logo" alt="Joann Ariola For City Council - District 32">
									    		</a>
									    	</div>
									    	<div class="nav-bar show-for-large">
									    		{{> nav}}
									    	</div>
									    </div>
									  </div>
									</div>
								</header>
					  		
					    	{{{body}}}

					    	<footer>
					    		<div class="grid-container container-inner">
						    		<div class="grid-x align-center">
									  	<div class="cell footer">
									  		<div class="paid-for">{{PAID_FOR}}</div>
													<div class="sharethis-inline-share-buttons share-buttons"></div>									  	
												</div>
									  </div>
						    	</div>
						    </footer>

					    </div>
					  </div>
					</div>
		    </div>
		  </div>
		</div>
   

		<script src="https://code.jquery.com/jquery-3.5.1.min.js" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>
		<script type='text/javascript' src='https://platform-api.sharethis.com/js/sharethis.js#property=60135a749dbb8e001c890945&product=sop' async='async'></script>
		<script type="text/javascript" src="//cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.min.js"></script>
		<script src="/javascripts/app.js?id=2"></script>

  </body>
</html>
